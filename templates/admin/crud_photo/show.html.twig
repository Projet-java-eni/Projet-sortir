{% extends 'base.html.twig' %}

{% block title %}Photo{% endblock %}

{% block body %}
    <h1>Photo</h1>

    <table class="table table-dark">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ photo.id }}</td>
            </tr>
            <tr>
                <th>Chemin_dd</th>
                <td>{{ photo.cheminDd }}</td>
            </tr>
            <tr>
                <th>IsProfilePicture</th>
                <td>{{ photo.isProfilePicture ? 'Yes' : 'No' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('crud_photo_index') }}">Retour</a>

    <a href="{{ path('crud_photo_edit', {'id': photo.id}) }}">Modifier</a>

    {{ include('admin/crud_photo/_delete_form.html.twig') }}
{% endblock %}
